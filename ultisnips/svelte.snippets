extends html
extends javascript

snippet if "if " b!
{#if ${1:condition}}
	${2:}
{/if}
endsnippet

snippet else "if/else " b!
{#if ${1:condition}}
	${2:}
{:else}
	${2:}
{/if}
endsnippet

snippet each "each " b!
{#each ${1:condition}}
	${2:}
{/each}
endsnippet

snippet key "key " b!
{#key ${1:value}}
	${2:}
{/key}
endsnippet

snippet debug "{@debug '' + ...}"
{@debug $0}
endsnippet

snippet debugs "{@debug ...}"
{@debug '$1' + $0}
endsnippet

snippet _ "{_('...')}"
{_('$0')}
endsnippet
